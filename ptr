-- Sliders
Tab1:CreateSlider({ Name = "🏃 Player Speed", Range = {16, 100}, Increment = 1, Suffix = "Speed", CurrentValue = 16, Flag = "PlayerSpeedSlider", Callback = function(Value)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
        player.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = Value
    end
end })

Tab1:CreateSlider({ Name = "🦘 Jump Power", Range = {50, 300}, Increment = 5, Suffix = "JumpPower", CurrentValue = 50, Flag = "JumpPowerSlider", Callback = function(Value)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChildOfClass("Humanoid") then
        player.Character:FindFirstChildOfClass("Humanoid").JumpPower = Value
    end
end })

