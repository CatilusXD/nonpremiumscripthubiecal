Tab2:CreateButton({
   Name = "Script VerifiedBest ESP Predictor Autobuild 99",
   Callback = function()
       -- load and run remote script when button clicked
       local ok, err = pcall(function()
           loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/7d8a2a1a9a562a403b52532e58a14065.lua"))()
       end)
       if not ok then
           Rayfield:Notify({
               Title = "Script Error",
               Content = "Failed to load script 1: "..tostring(err),
               Duration = 5
           })
       end
   end,
})


